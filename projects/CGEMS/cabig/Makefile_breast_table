snp_analysis_finding_fact.csv: snpanafindfact_breast.py \
                               /home/jacobske/projects/CGEMS/Scans/Breast/1/results.new/build6.out \
                               snp_map.csv
	python -u snpanafindfact_breast.py \
	-c 3318197 \
	-i /home/jacobske/projects/CGEMS/Scans/Breast/1/results.new/build6.out \
	-m snp_map.csv:dialect='csv' \
	-o snp_analysis_finding_fact.csv


study_participant.csv: studyparticipant_breast.py \
                       sample.def \
                       /home/wangzha/public/caBIG/hapmap_ceph_id.out
	python -u studyparticipant_breast.py \
	-c 1 \
	-s sample.def \
	-i /home/wangzha/public/caBIG/hapmap_ceph_id.out \
	-o study_participant.csv

specimen.csv: specimen_breast.py \
              sample.def \
              study_participant.csv
	python -u specimen_breast.py \
	-s sample.def \
	-i study_participant.csv:skip=1:index=1:dialect='csv' \
	-o specimen.csv

